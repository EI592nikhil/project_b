<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">My Courses</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Downloadable\Block\Customer\Products\ListProducts" name="downloadable_customer_products_list" template="Dentsu_Trainerb::customer/products/list.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>